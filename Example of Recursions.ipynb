   "metadata": {},
   "outputs": [],
   "source": [
    "def matryoshka(n):\n",
    "    if n == 1:\n",
    "        print('Middle')\n",
    "    else:\n",
    "        print('Top', n)\n",
    "        matryoshka(n-1)\n",
    "        print('Bottom', n)\n",
    "        "
   ]
  },
